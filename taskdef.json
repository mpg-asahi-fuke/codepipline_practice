{
  "executionRoleArn": "arn:aws:iam::135808928172:role/MyEcsTaskRole",
  "containerDefinitions": [
      {
          "name": "first",
          "image": "nginx",
          "essential": true,
          "portMappings": [
              {
                  "hostPort": 80,
                  "protocol": "tcp",
                  "containerPort": 80
              }
          ]
      }
  ],
  "requiresCompatibilities": [
      "FARGATE"
  ],
  "networkMode": "awsvpc",
  "cpu": "256",
  "memory": "512",
  "family": "service"
}
